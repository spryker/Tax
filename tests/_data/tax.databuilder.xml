<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="TaxRate">
        <property name="name" type="string" dataBuilderRule="unique()->sentence(2)" />
        <property name="rate" type="float" dataBuilderRule="randomFloat(2, 1, 1000)" />
    </transfer>

    <transfer name="TaxSet">
        <property name="name" type="string" dataBuilderRule="unique()->sentence(2)" />
    </transfer>

    <transfer name="Totals">
        <property name="grandTotal" type="int" dataBuilderRule="randomNumber(5)" />
    </transfer>

    <transfer name="Shipment">
        <property name="requestedDeliveryDate" type="string" dataBuilderRule="date('Y-m-d')" />
    </transfer>

</transfers>
